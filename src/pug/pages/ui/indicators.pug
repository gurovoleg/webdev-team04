extends ../../layouts/ui-layout.pug

block variables
	- var title = 'Индикаторы - UI-kit и HTML фреймворк - Документация';
	- var pageClass = 'body--dashboard';

block content
	include ../../sections/ui-nav-inside-ui.pug

	.main-wrapper
		.container.container--left
			.title.mt-50 Индикаторы
			.section-ui
				.indicators-section.dark
					.indicators-row
						+indicator("HTML5", 90,"#3aaa35")
						+indicator("CSS3", 90,"#3aaa35")
						+indicator("JS", 90,"#3aaa35")
						+indicator("Jquery", 90,"#3aaa35")
					.indicators-row
						+indicator("PHP", 90,"#009cf5")
						+indicator("MySql", 90,"#009cf5")
					.indicators-row
						+indicator("Git", 90,"#ffcc00")
						+indicator("Gulp", 90,"#ffcc00")
						+indicator("Bower", 90,"#ffcc00")
						+indicator("WebPack", 90,"#ffcc00")

			pre.
				.indicators-section.dark
					.indicators-row
						+indicator("HTML5", 90,"#3aaa35")
						+indicator("CSS3", 90,"#3aaa35")
						+indicator("JS", 90,"#3aaa35")
						+indicator("Jquery", 90,"#3aaa35")
					.indicators-row
						+indicator("PHP", 90,"#009cf5")
						+indicator("MySql", 90,"#009cf5")
					.indicators-row
						+indicator("Git", 90,"#ffcc00")
						+indicator("Gulp", 90,"#ffcc00")
						+indicator("Bower", 90,"#ffcc00")
						+indicator("WebPack", 90,"#ffcc00")
